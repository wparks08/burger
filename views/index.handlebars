<div class="row">
<div class="burger-column">
<ul class="burger-list">
{{#each notDevoured}}
    <li class="burger clearfix">
        {{this.burger_name}}
        <button class="burger-button" data-id={{this.id}}>
            Devour It!
        </button>
    </li>
{{/each}}
</ul>
</div>

<div class="burger-column">
    <ul class="burger-list">
        {{#each devoured}}
        <li class="burger clearfix">
            {{this.burger_name}}
        </li>
        {{/each}}
    </ul>
</div>
</div>